<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>üéâ Lifestyle Quiz ‚Äì Win a $10 Amazon Gift Card</title>
  <style>
    body {
      font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
      background: #f4f4f4;
      margin: 0;
      padding: 0;
      display: flex;
      justify-content: center;
      align-items: center;
      min-height: 100vh;
    }
    .container {
      background: #fff;
      max-width: 600px;
      width: 90%;
      padding: 30px;
      border-radius: 12px;
      box-shadow: 0 5px 25px rgba(0,0,0,0.1);
      text-align: center;
    }
    h1 {
      color: #ff4081;
      font-size: 28px;
      margin-bottom: 10px;
    }
    .timer {
      font-size: 18px;
      color: red;
      font-weight: bold;
      margin-bottom: 10px;
    }
    img {
      max-width: 250px;
      margin: 10px auto;
      border-radius: 10px;
    }
    .progress-bar {
      width: 100%;
      background: #ddd;
      height: 10px;
      border-radius: 5px;
      margin: 15px 0;
    }
    .progress {
      height: 10px;
      width: 0%;
      background: #28a745;
      border-radius: 5px;
      transition: width 0.3s;
    }
    .question {
      display: none;
      margin: 20px 0;
    }
    .question.active {
      display: block;
    }
    button {
      background: #28a745;
      color: #fff;
      padding: 12px 25px;
      border: none;
      border-radius: 8px;
      font-size: 18px;
      cursor: pointer;
      margin-top: 15px;
      display: block;
      width: 100%;
      margin-bottom: 10px;
    }
    button:hover {
      background: #218838;
    }
    .result {
      display: none;
    }
  </style>
</head>
<body>
  <div class="container">
    <h1>üéÅ Take the Lifestyle Quiz & Win a $10 Amazon Gift Card!</h1>
    <div class="timer">‚è≥ Hurry! Time left: <span id="time">3:00</span></div>
    <img src="data:image/png;base64," alt="Gift Card" />

    <div class="progress-bar"><div class="progress"></div></div>

    <div id="quiz">
      <div class="question active" data-index="0">
        <p><strong>Question 1:</strong> How often do you exercise each week?</p>
        <button class="answer">0-1 times</button>
        <button class="answer">2-3 times</button>
        <button class="answer">4+ times</button>
      </div>
      <div class="question" data-index="1">
        <p><strong>Question 2:</strong> What's your favorite way to relax?</p>
        <button class="answer">Watching movies</button>
        <button class="answer">Reading</button>
        <button class="answer">Spending time outdoors</button>
      </div>
      <div class="question" data-index="2">
        <p><strong>Question 3:</strong> How many hours of sleep do you usually get?</p>
        <button class="answer">Less than 6 hours</button>
        <button class="answer">6-8 hours</button>
        <button class="answer">More than 8 hours</button>
      </div>
      <div class="question" data-index="3">
        <p><strong>Question 4:</strong> What's your go-to beverage in the morning?</p>
        <button class="answer">Coffee</button>
        <button class="answer">Tea</button>
        <button class="answer">Juice or Water</button>
      </div>
      <div class="question" data-index="4">
        <p><strong>Question 5:</strong> How do you prefer to spend weekends?</p>
        <button class="answer">Traveling</button>
        <button class="answer">Relaxing at home</button>
        <button class="answer">Going out with friends</button>
      </div>
      <div class="question" data-index="5">
        <p><strong>Question 6:</strong> Which device do you use most often?</p>
        <button class="answer">Phone</button>
        <button class="answer">Laptop</button>
        <button class="answer">Tablet</button>
      </div>
      <div class="question" data-index="6">
        <p><strong>Question 7:</strong> Do you enjoy trying new foods?</p>
        <button class="answer">Yes, always!</button>
        <button class="answer">Sometimes</button>
        <button class="answer">Not really</button>
      </div>
    </div>

    <div class="result">
      <h2>üéâ Congratulations!</h2>
      <p>You completed the lifestyle quiz successfully.  
      To claim your <strong>$10 Amazon gift card</strong>, please complete one quick task on the next page.</p>
      <button onclick="window.location.href='https://unlockcontent.net/cl/i/lk21qj'">üëâ Claim Your Prize</button>
    </div>
  </div>

  <script>
    const answers = document.querySelectorAll('.answer');
    const questions = document.querySelectorAll('.question');
    const result = document.querySelector('.result');
    const progress = document.querySelector('.progress');
    let currentIndex = 0;

    // Progress bar update
    function updateProgress() {
      const percent = ((currentIndex) / questions.length) * 100;
      progress.style.width = percent + '%';
    }
    updateProgress();

    answers.forEach(answer => {
      answer.addEventListener('click', () => {
        questions[currentIndex].classList.remove('active');
        currentIndex++;
        updateProgress();
        if (currentIndex < questions.length) {
          questions[currentIndex].classList.add('active');
        } else {
          progress.style.width = '100%';
          document.querySelector('#quiz').style.display = 'none';
          result.style.display = 'block';
        }
      });
    });

    // Countdown Timer (3 min)
    let time = 180;
    const timerEl = document.getElementById('time');
    const countdown = setInterval(() => {
      let min = Math.floor(time / 60);
      let sec = time % 60;
      timerEl.textContent = `${min}:${sec < 10 ? '0' + sec : sec}`;
      if (time <= 0) {
        clearInterval(countdown);
        document.querySelector('#quiz').style.display = 'none';
        result.style.display = 'block';
        result.querySelector('h2').textContent = "‚è∞ Time's up!";
      }
      time--;
    }, 1000);
  </script>
</body>
</html>
